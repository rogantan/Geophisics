<Window x:Class="Geophisics.AdminAddGeoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Geophisics"
        mc:Ignorable="d"
        Title="Добавить геофизика" Height="300" Width="300">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="Введите ФИО"/>
            <TextBox Text="{Binding Geo.Фио}"/>
            <TextBlock Text="Введите логин"/>
            <TextBox Text="{Binding Geo.Логин}"/>
            <TextBlock Text='Введите пароль'/>
            <TextBox Text="{Binding Geo.Пароль}"/>
            <Button Content="Сохранить" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
